{
    "name": "@kahi-ui/framework",
    "description": "Straight-forward Svelte UI for the Web",
    "author": "novacbn",
    "version": "0.2.7",
    "type": "module",
    "main": "./src/index.ts",
    "svelte": "./src/index.ts",
    "exports": {
        "./package.json": "./package.json",
        ".": "./src/index.ts",
        "./dist/kahi-ui.framework.css": "./dist/kahi-ui.framework.css"
    },
    "keywords": [
        "css",
        "css-framework",
        "html",
        "kahi-ui",
        "svelte",
        "svelte-components",
        "windicss"
    ],
    "files": [
        "dist"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/novacbn/kahi-ui"
    },
    "devDependencies": {
        "@babel/core": "^7.14.3",
        "@storybook/addon-actions": "^6.3.0-rc.11",
        "@storybook/addon-essentials": "^6.3.0-rc.11",
        "@storybook/addon-links": "^6.3.0-rc.11",
        "@storybook/svelte": "^6.3.0-rc.11",
        "@tsconfig/svelte": "^1.0.10",
        "babel-loader": "^8.2.2",
        "postcss": "^8.2.15",
        "postcss-cli": "^8.3.1",
        "postcss-import": "^14.0.2",
        "postcss-nesting": "^8.0.1",
        "postcss-replace": "^1.1.3",
        "postcss-windicss": "^0.1.0",
        "prettier": "^2.3.0",
        "storybook-builder-vite": "^0.0.9",
        "svelte-check": "^1.5.4",
        "svelte-loader": "^3.1.1",
        "svelte-preprocess": "^4.7.3",
        "tslib": "^2.2.0",
        "typescript": "^4.2.4",
        "windicss": "^3.0.9"
    },
    "dependencies": {
        "svelte": "^3.38.2"
    },
    "scripts": {
        "build:framework": "postcss --config postcss.config.cjs --output ./dist/kahi-ui.framework.css ./src/index.css",
        "build:storybook": "build-storybook",
        "dev:storybook": "start-storybook --ci -p 6006",
        "watch:framework": "postcss --config postcss.config.cjs --output ./dist/kahi-ui.framework.css --watch ./src/index.css"
    }
}
