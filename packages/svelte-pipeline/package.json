{
    "name": "svelte-pipeline",
    "version": "0.0.1",
    "author": "novacbn",
    "main": "lib/index.js",
    "module": "dist/esm/index.js",
    "global": "SveltePipeline",
    "typings": "types/index.d.ts",
    "files": [
        "dist",
        "lib",
        "types"
    ],
    "scripts": {
        "build:lib": "node ./scripts/build.js",
        "build:types": "tsc --emitDeclarationOnly",
        "build:typedoc": "typedoc --out ./dist/docs --exclude '**/index.ts' --stripInternal --target es6 --theme minimal --mode modules src",
        "test:types": "tsc --noEmit",
        "prebuild:clean": "rimraf ./dist;rimraf ./lib"
    },
    "dependencies": {
        "svelte": "^3.30.0",
        "url": "^0.11.0"
    },
    "devDependencies": {
        "@types/estree": "0.0.45",
        "@types/glob-to-regexp": "^0.4.0",
        "esbuild": "^0.7.5",
        "estree-walker": "^2.0.1",
        "glob": "^7.1.6",
        "magic-string": "^0.25.7",
        "prettier": "^2.0.5",
        "rimraf": "^3.0.2",
        "typedoc": "^0.19.2",
        "typescript": "^4.0.3"
    }
}
